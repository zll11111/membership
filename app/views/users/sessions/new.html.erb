<h2>登录</h2>

<%= simple_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <div class="form-inputs">
    <%= f.input :user_name, autofocus: true,validate: true%>
    <%= f.input :password, required: false %>
    <%= f.input :remember_me,label: "记住我", as: :boolean if devise_mapping.rememberable? %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, "登  录",class: "btn btn-primary", data:{disable_with:"正在登录..."} %>
  </div>
<% end %>
<script src="judge_show.js">
</script>
<%= render "users/shared/links" %>
